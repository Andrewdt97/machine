{
  "SmtModel": "Thot",
  "RuleEngine": "Transfer",
  "TextCorpus": "XForge",
  "DataAccess": "NoDb",
  "TranslationEngine": {
    "RootDir": "/var/lib/machine-web-api/engines/",
    "EngineCommitFrequency": "00:05",
    "InactiveEngineTimeout": "00:10"
  },
  "NoDbDataAccess": {
    "DataDir": "/var/lib/machine-web-api/data/"
  },
  "Security": {
    "InternalApiWhitelist": "127.0.0.1"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
